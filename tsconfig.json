{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["es2020", "DOM"],
		"module": "es2020",
		"moduleResolution": "node",
		"noEmit": true,
		"resolveJsonModule": true,
		"strict": true,
		"target": "ES2020",
		"types": ["express"]
	},
	"include": ["./**/*.d.ts", "./**/*.js"],
	"exclude": ["_aws", "node_modules"]
}
